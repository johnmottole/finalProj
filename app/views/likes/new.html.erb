<% @thequotes.each do |q| %>
	<div id="quote_show">
		<div id="timee"> 
			<%= q.user %> <br>
			<%= (q.created_at - (4*60*60)).month %> / <%= (q.created_at - (4*60*60)).day %> <br>
			<%= show_time(q) %>
		</div>
    	 "<%= q.words %>" <br>
   		  - <%= q.author %> <br>
    </div>
    
  <% end %>
    <%= link_to(image_tag("back2.png"), welcome_index_path) %>
